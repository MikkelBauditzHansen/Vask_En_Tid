@page
@model Vask_En_Tid.Pages.Shared.CreateBookingModel
@{
    @foreach (var booking in Model.Bookings)
    {
        <tr>
            <td>@booking.Resident.Name</td>
            <td>@booking.Machine.Name</td>
            <td>@booking.StartTime.ToString("dd-MM-yyyy HH:mm")</td>
            <td>@booking.EndTime.ToString("HH:mm")</td>
        </tr>
    }
}
